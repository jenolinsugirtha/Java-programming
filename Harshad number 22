import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;
public class HarshadNumber{
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int num= sc.nextInt();
        int a=num;
        int b=0;
        while(num>0)
        {
            b+=num % 10;
            num/= 10;
        }
        if (a%b==0)
        {
            System.out.println("Harshad Number");
        }
        else 
        {
            System.out.println("Not Harshad Number");
        }
    }
}
